{{template "admin" .}} 
{{define "page-title"}}
Reservations Calendar
{{end}}

{{define "content"}}
{{$now:=index .Data "now"}}
    <div class="col-md-12">
        <div class="text-center">
            <h3>{{formatDate $now "January"}} {{formatDate $now "2006"}}</h3>
        </div>
        <a class="btn vtn-sm btn-outline-secondary float-left" href="/admin/reservations-calendar?y={{index .StringMap "last_month_year"}}&m={{index .StringMap "last_month"}}">&lt;&lt;</a>
        <a class="btn vtn-sm btn-outline-secondary float-right" href="/admin/reservations-calendar?y={{index .StringMap "next_month_year"}}&m={{index .StringMap "next_month"}}">&gt;&gt;</a>

        
    </div>



{{end}}